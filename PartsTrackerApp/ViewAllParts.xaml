<Window x:Name="viewPartsWindow" x:Class="PartsTrackerApp.ViewAllParts"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PartsTrackerApp"
        mc:Ignorable="d"
        Title="Will's Parts Tracker" Height="450" Width="800" Background="#FF4365D6" WindowStartupLocation="CenterScreen">
    <Grid Loaded="Grid_Loaded">
        <Label x:Name="titleLabel" Content="View All Parts" Height="54" Margin="251,39,251,0" VerticalAlignment="Top" FontSize="25" FontWeight="SemiBold" HorizontalContentAlignment="Center" FontFamily="Sylfaen" Foreground="#FFFDF20C"/>
        <ListBox x:Name="displayListBox" Height="183" VerticalAlignment="Top" Margin="198,117,198,0"/>
        <Button x:Name="backButton" Content="Back" Margin="328,323,328,0" VerticalAlignment="Top" Height="57" RenderTransformOrigin="0.524,0.604" Click="BackButton_Click"/>

    </Grid>
</Window>
